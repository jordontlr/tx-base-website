<can-import from="~/pages/page-shop/checkout.component!" />
<can-import from="grid-component" />
<can-import from="~/attrs/modal-scrollstop.attr" />
<can-import from="~/attrs/selectpicker.attr" />

<br>
<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <div class="jumbotron">
        <h1>Shop</h1>
        <p>Some things to buy.</p>

        {{#if filterTags}}
          <p>For Posts with Tag <strong>{{filterTags}}</strong>.</p>
          <p><button class="btn btn-primary" type="button" on:click="clearFilterTags()">Clear Tag Filter</button></p>
        {{/if}}

        {{#if scope.root.userCart.initiatedPayment}}
          <p class="text-danger">Payment is being processed. You can no longer edit your shopping cart until payment is complete.</p>
        {{/if}}

        {{#if userCart.items.length}}
          <p>
            You currently have <strong>{{userCart.items.length}}</strong> products in your shopping cart.
            <button class="btn btn-primary btn-sm inline-block" type="button" data-toggle="collapse" data-target="#shoppingCart" aria-expanded="false">
              Toggle Cart Details <i class="fa fa-shopping-cart" aria-hidden="true"></i>
            </button>
            <button class="btn btn-warning btn-sm inline-block" type="button" on:click="checkout()">
              Checkout <i class="fa fa-shopping-cart" aria-hidden="true"></i>
            </button>
          </p>

          <div class="collapse" id="shoppingCart">
            <br>
            <div class="table-responsive">
              <table class="table table-striped">
                <thead>
                <tr>
                  <th class="text-nowrap h4">Product Category / Name</th>
                  <th class="text-nowrap h4 text-center">Unit Price</th>
                  <th class="text-nowrap h4 text-center">Quantity</th>
                    {{^if scope.root.userCart.initiatedPayment}}
                      <th class="text-nowrap h4 text-center">Edit</th>
                    {{/if}}
                  <th class="text-nowrap h4 text-right">Total</th>
                </tr>
                </thead>
                <tbody>
                {{#each (userCart.items)}}
                  <tr>
                    <td><span class="mouse-pointer hover-color" on:click="scope.root.openShopModal(this)">{{#if category}}{{category}} / {{/if}} {{product}}</span></td>
                    <td class="text-center">{{numberFormat price}}</td>
                    <td class="text-center">{{quantity}}</td>
                    {{^if scope.root.userCart.initiatedPayment}}
                      <td class="text-center text-nowrap">
                        <div class="btn-group" role="group" style="min-width: 66px;">
                          <button class="btn btn-xs btn-default" type="button" on:click="scope.root.quantityUp(this)">
                            <i class="fa fa-plus" aria-hidden="true"></i>
                          </button>
                          <button class="btn btn-xs btn-default" type="button" on:click="scope.root.quantityDown(this)">
                            <i class="fa fa-minus" aria-hidden="true"></i>
                          </button>
                          <button class="btn btn-xs btn-danger" type="button" on:click="scope.root.quantityRemove(this)">
                            <i class="fa fa-trash" aria-hidden="true"></i>
                          </button>
                        </div>
                      </td>
                    {{/if}}
                    <td class="text-right">{{numberFormat total}}</td>
                  </tr>
                {{/each}}
                <tr>
                  <td colspan="{{^if scope.root.userCart.initiatedPayment}}4{{else}}3{{/if}}" class="h4">&nbsp;</td>
                  <td class="text-nowrap h4 text-right">{{numberFormat userCart.cartTotal}}</td>
                </tr>
                </tbody>
              </table>
            </div>
          </div>
        {{/if}}

      </div>
    </div>
  </div>
</div>

{{^if scope.root.loadingShop}}
  <div class="container">

    <div class="row">

      <grid-component rows:bind="rows" pagination:bind="pagination" on:onpage="loadPage()">

        {{^if scope.root.filterTags}}
          <div class="col-sm-3">
            <div class="list-group">
              <div class="list-header list-group-item h4 no-margin-top">Product Categories</div>
              {{#each (scope.root.categories)}}
                <a href="javascript:void(0)" on:click="scope.root.changeCategory(item)" class="list-group-item {{#eq scope.root.filterCategory item}} active{{/eq}}">
                  {{item}}
                  <span class="badge">{{count}}</span>
                </a>
              {{/each}}
            </div>
          </div>
        {{/if}}

        <div class="{{^if scope.root.filterTags}}col-sm-9{{else}}col-sm-12{{/if}}">
          {{#if scope.root.filterCategory}}
            <h1 class="no-margin-top margin-bottom">{{scope.root.filterCategory}}</h1>
          {{/if}}
          <div class="row">

            <!-- filter sort type -->
            <div class="col-sm-6 col-md-6 col-lg-6">
              <div class="btn-group dropdown">
                <button type="button" class="btn btn-default">Sort By {{capFirst scope.root.sortType}}</button>
                <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><span class="caret"></span></button>
                <ul class="dropdown-menu">
                  <li><a href="javascript:void(0)" on:click="scope.root.changeSortType('product')">Product</a></li>
                  <li><a href="javascript:void(0)" on:click="scope.root.changeSortType('price')">Price</a></li>
                </ul>
              </div>
              <button class="btn btn-default" on:click="scope.root.changeSortDirection()"><i class="fa fa-arrow-{{scope.root.sortDirection}}" aria-hidden="true"></i></button>
            </div>

            <!-- change display type -->
            <div class="hidden-xs col-sm-6 col-md-6 col-lg-6">
              <div class="text-right">
                <button class="btn {{#eq scope.root.displayType 'large'}}btn-primary{{else}}btn-default{{/eq}}" on:click="scope.root.changeDisplayType('large')"><i class="fa fa-th-large" aria-hidden="true"></i></button>
                <button class="btn {{#eq scope.root.displayType 'list'}}btn-primary{{else}}btn-default{{/eq}}" on:click="scope.root.changeDisplayType('list')"><i class="fa fa-th-list" aria-hidden="true"></i></button>
              </div>
            </div>

          </div>
          <hr>
          <div class="row">
            {{^if scope.root.loadingShopList}}
              {{#if rows.length}}
                {{#each (rows)}}
                  {{#eq scope.root.displayType 'list'}}
                    <div class="col-sm-12">
                      <div class="row">
                        <div class="col-sm-3">
                          <div class="mouse-pointer {{#if this.imageData[1]}}hover-change-image{{/if}}" on:click="scope.root.openShopModal(this)">
                            <div class="embed-responsive embed-responsive-4by3 img-rounded background-image-css {{#if this.imageData[1]}}image-one{{/if}}" {{#if this.imageData[0]}}style="background-image: url({{this.imageData[0]}})"{{else}}style="background-image: url(data:image/jpeg;base64,);"{{/if}}></div>
                            {{#if this.imageData[1]}}<div class="embed-responsive embed-responsive-4by3 img-rounded background-image-css image-two" style="background-image: url({{this.imageData[1]}})"></div>{{/if}}
                          </div>
                        </div>
                        <div class="col-sm-9">
                          <div class="hidden visible-xs"><br></div>
                          <small class="pull-right">{{numberFormat price}}</small>
                          <h4 class="no-margin-top one-line mouse-pointer hover-color" on:click="scope.root.openShopModal(this)">{{product}}</h4>
                          <p class="one-line">{{short}}</p>
                          <div class="hidden visible-lg"><br><br></div>
                          <div class="hidden visible-md"><br></div>
                          <div class="text-right">
                            <a href="javascript:void(0)" class="btn btn-primary" role="button" on:click="scope.root.openShopModal(this)">View Details</a>
                            {{^if scope.root.userCart.initiatedPayment}}
                              <a href="javascript:void(0)" class="btn btn-primary" role="button" on:click="scope.root.addToCart(this)">Add 1 to Cart <i class="fa fa-shopping-cart" aria-hidden="true"></i></a>
                            {{/if}}
                          </div>
                        </div>
                      </div>
                      <hr>
                    </div>
                  {{/eq}}
                  {{#eq scope.root.displayType 'large'}}
                    <div class="col-sm-6 col-md-6 col-lg-4">
                      <div class="thumbnail">
                        <div class="mouse-pointer {{#if this.imageData[1]}}hover-change-image{{/if}}" on:click="scope.root.openShopModal(this)">
                          <div class="embed-responsive embed-responsive-4by3 img-rounded background-image-css {{#if this.imageData[1]}}image-one{{/if}}" {{#if this.imageData[0]}}style="background-image: url({{this.imageData[0]}})"{{else}}style="background-image: url(data:image/jpeg;base64,);"{{/if}}></div>
                          {{#if this.imageData[1]}}<div class="embed-responsive embed-responsive-4by3 img-rounded background-image-css image-two" style="background-image: url({{this.imageData[1]}})"></div>{{/if}}
                        </div>
                        <div class="caption">
                          <small class="pull-right">{{numberFormat price}}</small>
                          <h4 class="one-line mouse-pointer hover-color" on:click="scope.root.openShopModal(this)">{{product}}</h4>
                          <p class="one-line">{{short}}</p>
                          <div class="btn-group btn-group-justified" role="group">
                            <a href="javascript:void(0)" class="btn btn-primary" role="button" on:click="scope.root.openShopModal(this)">View Details</a>
                            {{^if scope.root.userCart.initiatedPayment}}
                              <a href="javascript:void(0)" class="btn btn-primary" role="button" on:click="scope.root.addToCart(this)">Add 1 to Cart</a>
                            {{/if}}
                          </div>
                        </div>
                      </div>
                    </div>
                  {{/eq}}
                {{/each}}
              {{else}}
                NONE
              {{/if}}
            {{else}}
              <div class="text-center top-bottom-padding"><i class="fa fa-spin fa-spinner fa-5x"></i></div>
            {{/if}}
          </div>

          <div class="row">

            <!-- pagination -->
            <div class="col-sm-9 col-md-9 col-lg-9">
              {{#if hasPages}}
                <nav aria-label="Page navigation">
                  <button class="btn btn-default" {{^if isPrevActive}}disabled{{/if}} on:click="prev()"><i class="fa fa-angle-double-left" aria-hidden="true"></i></button>
                  {{#each (pages)}}
                    <button class="btn {{#if isActive}}btn-primary{{else}}btn-default{{/if}}" on:click="../changePage(pageNumber)">{{pageTitle}}</button>
                  {{/each}}
                  <button class="btn btn-default" {{^if isNextActive}}disabled{{/if}} on:click="next()"><i class="fa fa-angle-double-right" aria-hidden="true"></i></button>
                </nav>
              {{/if}}
            </div>

            <!-- display view number -->
            {{#if rows.length}}
              <div class="col-sm-3 col-md-3 col-lg-3">
                <div class="pull-right">
                  <div class="btn-group dropup">
                    <button type="button" class="btn btn-default">View {{scope.root.pagination.limit}}</button>
                    <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><span class="caret"></span></button>
                    <ul class="view-btn dropdown-menu">
                      <li><a href="javascript:void(0)" on:click="scope.root.changeLimit('6')">6</a></li>
                      <li><a href="javascript:void(0)" on:click="scope.root.changeLimit('12')">12</a></li>
                      <li><a href="javascript:void(0)" on:click="scope.root.changeLimit('24')">24</a></li>
                    </ul>
                  </div>
                </div>
              </div>
            {{/if}}

          </div>
        </div>
      </grid-component>
    </div>

  </div>
{{else}}
  <div class="text-center top-bottom-padding"><i class="fa fa-spin fa-spinner fa-5x"></i></div>
{{/if}}

<div class="modal fade" id="view-item-details" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" modal-scrollstop>
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">{{#if viewShopItem.category}}{{viewShopItem.category}} / {{/if}} {{viewShopItem.product}}</h4>
      </div>
      <div class="modal-body">
        <div class="row">
          {{#if viewShopItem.imageData.length}}
            {{#eq viewShopItem.imageData.length 1}}
              <div class="hidden-xs col-sm-12 col-md-6">
                <div class="thumbnail {{^if viewShopItem.description}}no-margin-bottom{{/if}}">
                  <div class="embed-responsive embed-responsive-4by3 img-rounded background-image-css" style="background-image: url({{viewShopItem.imageData[0]}})"></div>
                </div>
              </div>
            {{else}}
              <div class="col-sm-4 col-md-2">
                {{#each viewShopItem.imageData}}
                  {{#if this}}
                    <div class="thumbnail">
                      <div class="embed-responsive embed-responsive-4by3 img-rounded background-image-css" style="background-image: url({{this}})"></div>
                    </div>
                  {{/if}}
                {{/each}}
              </div>
              <div class="hidden-xs col-sm-8 col-md-5">
                <div class="thumbnail {{^if viewShopItem.description}}no-margin-bottom{{/if}}">
                  <div class="embed-responsive embed-responsive-4by3 img-rounded background-image-css" style="background-image: url({{viewShopItem.imageData[0]}})"></div>
                </div>
              </div>
            {{/eq}}
          {{/if}}
          <div class="col-sm-12 {{#if viewShopItem.imageData.length}}{{#eq viewShopItem.imageData.length 1}}col-md-6{{else}}col-md-5{{/eq}}{{/if}}">
            <h3 class="no-margin-top">{{viewShopItem.short}}</h3>
            {{#if viewShopItem.brand}}<h5 class="text-muted">Brand: {{viewShopItem.brand}}</h5>{{/if}}
            {{#if viewShopItem.sku}}<h5 class="text-muted">SKU: {{viewShopItem.sku}}</h5>{{/if}}
            <h1 class="text-primary">{{numberFormat viewShopItem.price}}</h1>

            {{^if viewShopItem.addedToCart}}
              {{^if userCart.initiatedPayment}}
                <hr>
                <div class="row">
                  <div class="col-xs-6">
                    <div class="input-group">
                          <span class="input-group-btn">
                            <button class="btn btn-default" type="button" on:click="quantityUp()">
                              <i class="fa fa-plus" aria-hidden="true"></i>
                            </button>
                          </span>
                      <input type="number" class="form-control" placeholder="Quantity" value:bind="viewShopItem.quantity">
                      <span class="input-group-btn">
                            <button class="btn btn-default" type="button" on:click="quantityDown()">
                              <i class="fa fa-minus" aria-hidden="true"></i>
                            </button>
                          </span>
                    </div>
                  </div>
                  <div class="col-xs-6">
                    <button type="button" class="btn btn-primary btn-block" role="button" on:click="addToCart(viewShopItem)">Add to Cart <i class="fa fa-shopping-cart" aria-hidden="true"></i></button>
                  </div>
                </div>
              {{/if}}
            {{else}}
              <hr>
              <h4>
                You Have <strong class="text-primary">{{viewShopItem.quantity}}</strong> in Your Cart
                {{^if userCart.initiatedPayment}}
                  <div class="btn-group inline-block" role="group">
                    <button class="btn btn-sm btn-default" type="button" on:click="quantityUp()">
                      <i class="fa fa-plus" aria-hidden="true"></i>
                    </button>
                    <button class="btn btn-sm btn-default" type="button" on:click="quantityDown()">
                      <i class="fa fa-minus" aria-hidden="true"></i>
                    </button>
                    <button class="btn btn-sm btn-danger" type="button" on:click="quantityRemove()">
                      <i class="fa fa-trash" aria-hidden="true"></i>
                    </button>
                  </div>
                {{/if}}
              </h4>
            {{/if}}

            <hr>

            {{#if viewShopItem.tags.length}}
              <span>Tags: </span>
              {{#each viewShopItem.tags}}
                <a href="javascript:void(0)" on:click="scope.root.changeTags(this)">{{this}}</a>
              {{/each}}
              <hr>
            {{/if}}

          </div>
        </div>
        {{#if viewShopItem.description}}
          <div class="well no-margin-bottom">
            <div class="row">
              <div class="col-sm-12">
                <ul class="nav nav-pills nav-justified" role="tablist">
                  <li role="presentation" class="active"><a href="#desc" aria-controls="home" role="tab" data-toggle="tab"><strong>Description</strong></a></li>
                  {{#if viewShopItem.content}}<li role="presentation"><a href="#content" aria-controls="profile" role="tab" data-toggle="tab"><strong>Box Contents</strong></a></li>{{/if}}
                </ul>

                <br>

                <div class="tab-content">
                  <div role="tabpanel" class="tab-pane active" id="desc">{{{viewShopItem.description}}}</div>
                  {{#if viewShopItem.content}}<div role="tabpanel" class="tab-pane" id="content">{{{viewShopItem.content}}}</div>{{/if}}
                </div>
              </div>
            </div>
          </div>
        {{/if}}

      </div>
    </div>
  </div>
</div>

<page-checkout userCart:from="userCart" />