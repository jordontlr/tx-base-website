{
  "name": "tx",
  "version": "0.1.0",
  "description": "An awesome DoneJS app",
  "homepage": "",
  "engines": {
    "node": "9.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donejs-user/tx.git"
  },
  "author": {
    "name": "Marc Godard",
    "email": "me@marcgodard.com",
    "url": ""
  },
  "private": true,
  "scripts": {
    "ready:staging": "npm run remove-build && node build",
    "remove-build": "make remove-build",
    "develop": "done-serve --static --develop --port 8080",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "start": "done-serve --port $PORT",
    "staging": "npm run release:staging:patch",
    "release:staging:patch": "npm test && npm run ready:staging && npm version patch && make release-staging",
    "test": "npm run lint && npm run testee",
    "testee": "testee test.html --browsers firefox --reporter Spec",
    "build": "node build"
  },
  "main": "tx/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "",
    "donejs-app"
  ],
  "steal": {
    "main": "tx/index.stache!done-autorender/src/no-zone",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register",
      "node_modules/steal-conditional/conditional"
    ],
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": ""
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-select": "^1.12.4",
    "bootstrap-tagsinput": "^0.7.1",
    "can-component": "^4.0.2",
    "can-connect": "^2.0.2",
    "can-connect-feathers": "^4.0.1",
    "can-define": "^2.0.1",
    "can-route": "^4.1.0",
    "can-route-pushstate": "^4.0.1",
    "can-set": "^1.5.1",
    "can-stache": "^4.1.0",
    "can-stache-route-helpers": "^0.1.4",
    "can-view-autorender": "^4.0.0",
    "can-zone": "^0.6.13",
    "cropit": "git+https://git@github.com/MarcGodard/cropit.git",
    "done-autorender": "^2.0.0",
    "done-component": "^2.0.0",
    "done-css": "^3.0.2",
    "done-serve": "^2.0.0",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "feathers": "^2.2.0",
    "feathers-authentication-client": "^0.3.3",
    "feathers-hooks": "^2.0.2",
    "feathers-rest": "^1.8.0",
    "feathers-socketio": "^2.0.0",
    "fingerprintjs2": "^1.6.1",
    "font-awesome": "^4.7.0",
    "generator-donejs": "^2.1.0",
    "grid-component": "^0.11.0",
    "jquery": "^3.2.1",
    "js-cookie": "^2.2.0",
    "moment": "^2.18.1",
    "quill": "^1.3.2",
    "socket.io-client": "^2.0.3",
    "steal": "^1.6.5",
    "steal-less": "^1.2.2",
    "steal-stache": "^4.0.1",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "can-debug": "^1.0.0",
    "can-fixture": "^2.0.2",
    "chai": "^4.1.2",
    "donejs-cli": "^2.0.0",
    "funcunit": "^3.2.0",
    "standard": "^10.0.3",
    "steal-conditional": "^1.0.0",
    "steal-mocha": "^1.0.0",
    "steal-tools": "^1.11.3",
    "testee": "^0.7.0"
  }
}
